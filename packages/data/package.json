    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@sports-bar/logger": "*",
    "drizzle-orm": "^0.45.0"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "typescript": "^5.9.2"
  }
