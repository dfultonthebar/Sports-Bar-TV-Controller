{
  "name": "@sports-bar/data",
  "version": "1.0.0",
  "description": "Data layer utilities for Sports Bar TV Controller",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "private": true,
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
