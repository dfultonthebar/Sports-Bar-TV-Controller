{
  "name": "sports-bar-ai-assistant",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "rm -rf .next node_modules/.cache",
    "clean:all": "rm -rf .next node_modules",
    "rebuild": "npm run clean && npm run build",
    "rebuild:fresh": "npm run clean:all && npm ci && npm run build",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --config jest.config.js --watch",
    "test:integration": "jest --config jest.config.integration.js",
    "test:integration:watch": "jest --config jest.config.integration.js --watch",
    "test:hardware": "jest --config jest.config.integration.js tests/integration/hardware.test.ts",
    "test:api": "jest --config jest.config.integration.js tests/integration/api.test.ts",
    "test:database": "jest --config jest.config.integration.js tests/integration/database.test.ts",
    "test:matrix": "jest --config jest.config.integration.js tests/integration/matrix.test.ts",
    "test:firetv": "jest --config jest.config.integration.js tests/integration/firetv.test.ts",
    "test:scenarios": "jest --config jest.config.integration.js tests/scenarios",
    "test:all": "npm run test && npm run test:integration",
    "test:coverage": "jest --config jest.config.js --coverage",
    "test-ai": "python3 ai-analysis/test_ai_system.py",
    "ai-status": "python3 ai-analysis/check_status.py",
    "cleanup-logs": "./scripts/cleanup-logs.sh",
    "force-db-fix": "tsx scripts/force_db_creation.ts",
    "build-knowledge-base": "tsx scripts/build-knowledge-base.ts",
    "verify-ai": "tsx scripts/verify-ai-system.ts",
    "setup:ai": "node ai-assistant/setup.js",
    "setup:ai:bash": "bash ai-assistant/setup.sh",
    "check:ai": "node ai-assistant/check-dependencies.js",
    "rag:scan": "tsx scripts/scan-docs.ts",
    "rag:scan:clear": "tsx scripts/scan-docs.ts --clear",
    "rag:test": "tsx scripts/test-rag.ts",
    "memory:snapshot": "tsx scripts/memory-bank.ts snapshot",
    "memory:restore": "tsx scripts/memory-bank.ts restore",
    "memory:watch": "tsx scripts/memory-bank.ts watch",
    "memory:stop": "tsx scripts/memory-bank.ts stop",
    "memory:list": "tsx scripts/memory-bank.ts list",
    "memory:stats": "tsx scripts/memory-bank.ts stats",
    "type-check": "./scripts/type-check.sh",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx,json}'",
    "format:check": "prettier --check 'src/**/*.{ts,tsx,js,jsx,json}'",
    "test:vitest": "./scripts/run-tests.sh",
    "test:vitest:coverage": "./scripts/run-tests.sh --coverage",
    "quality-check": "./scripts/quality-check.sh",
    "pre-commit": "./scripts/pre-commit.sh"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2",
    "@types/opossum": "^8.1.9",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^4.0.7",
    "@vitest/ui": "^4.0.7",
    "autoprefixer": "^10.4.21",
    "better-sqlite3": "^11.10.0",
    "drizzle-kit": "^0.31.5",
    "eslint": "^9.36.0",
    "eslint-config-next": "^15.5.4",
    "jest": "^30.2.0",
    "lucide-react": "^0.544.0",
    "node-mocks-http": "^1.17.2",
    "playwright": "^1.56.1",
    "postcss": "^8.5.6",
    "supertest": "^7.1.4",
    "tailwindcss": "^3.4.18",
    "ts-jest": "^29.4.5",
    "tsx": "^4.19.0",
    "typescript": "^5.9.2",
    "vitest": "^4.0.7"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.65.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@types/formidable": "^3.4.5",
    "@types/multer": "^2.0.0",
    "@types/node-cron": "^3.0.11",
    "@types/uuid": "^10.0.0",
    "axios": "^1.12.2",
    "bcryptjs": "2.4.3",
    "cheerio": "^1.1.2",
    "chokidar": "^4.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cron-parser": "^5.4.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.6",
    "next": "^15.5.6",
    "next-auth": "^4.24.13",
    "next-pwa": "^5.6.0",
    "node-cron": "^4.2.1",
    "node-ssdp": "^4.0.1",
    "openai": "^6.2.0",
    "opossum": "^9.0.0",
    "pdf-parse": "^1.1.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hot-toast": "^2.4.1",
    "recharts": "^3.2.1",
    "sharp": "^0.34.4",
    "tailwind-merge": "^3.3.1",
    "tesseract.js": "^6.0.1",
    "uuid": "^13.0.0",
    "zod": "^3.25.76"
  },
  "optionalDependencies": {
    "isolated-vm": "^6.0.1"
  }
}
